{
    "name": "electron-quick-start",
    "version": "1.0.0",
    "description": "A minimal Electron application",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "packager": "electron-packager . test --win --out ./outApp --arch=x64 --asar--version 1.4.14 --overwrite --ignore=dist --icon=./images/icon.ico",
        "pack:osx": "electron-packager . test --out=dist/osx --asar --platform=darwin --arch=x64 --overwrite --ignore=.sass-cache --ignore=sass --icon=images/icon.ico",
        "pack:win32": "electron-packager . test --out=dist/win --asar --platform=win32 --arch=ia32 --overwrite --ignore=.sass-cache --ignore=sass",
        "pack:win64": "electron-packager . test --out=dist/win --asar --platform=win32 --arch=x64 --version=0.36.2 --overwrite --ignore=.sass-cache --ignore=sass",
        "build": "npm run pack:osx && npm run pack:win32 && npm run pack:win64"
    },
    "repository": "https://github.com/electron/electron-quick-start",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "GitHub",
    "license": "CC0-1.0",
    "devDependencies": {
        "electron": "~1.7.8",
        "electron-packager": "^10.1.0"
    }
}