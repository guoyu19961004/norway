{
    "name": "Norway-envrionment",
    "version": "1.0.0",
    "description": "A Norway Project application",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "pack:osx": "electron-packager ./app test --out=dist --asar --platform=darwin --arch=x64 --overwrite --ignore=.sass-cache --ignore=sass --ignore=.rb --icon=app/images/icon.icns",
        "pack:win32": "electron-packager ./app test --out=dist --asar --platform=win32 --arch=ia32 --overwrite --ignore=.sass-cache --ignore=sass --ignore=.rb --icon=app/images/icon.ico",
        "pack:win64": "electron-packager ./app test --out=dist --platform=win32 --arch=x64 --overwrite --ignore=.sass-cache --ignore=sass --ignore=.rb --icon=app/images/icon.ico",
        "build": "npm run pack:osx && npm run pack:win32 && npm run pack:win64"
    },
    "repository": "hhttps://github.com/guoyu19961004/norway",
    "keywords": [
        "Norway",
        "project"
    ],
    "author": "guoyu",
    "license": "CC0-1.0",
    "devDependencies": {
        "electron": "~1.7.8",
        "electron-packager": "^10.1.0"
    }
}